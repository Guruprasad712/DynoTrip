# Backend environment example for DynoTrip
# Copy to .env and fill values locally. Do NOT commit your real .env.

# Google/Vertex credentials
GOOGLE_APPLICATION_CREDENTIALS=/absolute/path/to/firestore-sa.json
PROJECT_ID=your-gcp-project-id
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_MODEL=gemini-2.5-flash

# If using API key auth to Gemini (preferred for local dev), set one of these:
GEMINI_API_KEY=your_gemini_api_key
VERTEX_API_KEY=your_vertex_api_key

# MCP tools server URL (required for real generation)
# e.g. http://127.0.0.1:9000/mcp
MCP_SERVER_URL=
